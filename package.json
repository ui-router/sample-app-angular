{
  "name": "sample-app-angular",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --prod --sm",
    "build": "ng build --prod --sm",
    "test": "run-s build e2e",
    "e2e": "run-p -r browser-sync cypress",
    "browser-sync": "browser-sync start --port 4000 --server dist --no-open",
    "cypress": "wait-on tcp:4000 && cypress run",
    "gh-pages": "ng build --base-href=/sample-app-angular/ && shx rm -rf pages && shx mkdir pages && cd pages && git init && git remote add pages git@github.com:ui-router/sample-app-angular.git && git fetch pages && git checkout gh-pages && git rm -rf * && shx mv ../dist/* . && git add . && git commit -m 'Update gh-pages' . && git push && cd .. && shx rm -rf pages"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "5.1.2",
    "@angular/compiler": "5.1.2",
    "@angular/core": "5.1.2",
    "@angular/forms": "5.1.2",
    "@angular/http": "5.1.2",
    "@angular/platform-browser": "5.1.2",
    "@angular/platform-browser-dynamic": "5.1.2",
    "@angular/router": "5.1.2",
    "@ngtools/webpack": "1.9.2",
    "@uirouter/angular": "1.0.0-rc.2",
    "@uirouter/visualizer": "5.0.6",
    "bootstrap": "^3.3.7",
    "core-js": "2.5.3",
    "font-awesome": "^4.7.0",
    "rxjs": "5.5.6",
    "ts-helpers": "^1.1.1",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.6.2",
    "@angular/compiler-cli": "5.1.2",
    "@types/jasmine": "2.8.2",
    "@types/node": "^8.5.2",
    "@types/whatwg-fetch": "0.0.33",
    "browser-sync": "^2.21.0",
    "codelyzer": "~4.0.2",
    "cypress": "^1.4.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "0.6.0",
    "npm-run-all": "^4.1.2",
    "protractor": "5.2.2",
    "shx": "^0.2.2",
    "ts-node": "4.0.2",
    "tslint": "^5.8.0",
    "typescript": "~2.5.0",
    "wait-on": "^2.0.2"
  }
}
